{
  "name": "kkeiper1103/phalcon-site",
  "description": "A Phalcon test site to learn how to use Phalcon with Docker",
  "minimum-stability": "dev",

  "config": {
    "platform": {
      "php": "8.3",
      "ext-phalcon": "5.8"
    }
  },

  "autoload": {
    "psr-4": {
      "App\\": "src/"
    },
    "files": [
      "src/functions.php",
      "src/helpers.php"
    ]
  },

  "require": {
    "phalcon/ide-stubs": "^5.8",
    "vlucas/phpdotenv": "^5.6@dev",
    "psr/http-factory": "^1.1",
    "psr/http-message": "^2.0@dev"
  },
  "require-dev": {
    "phalcon/devtools": "^5.0.x-dev",
    "phalcon/migrations": "^3.0"
  }
}
